#include<stdio.h>
//有1、2、3、4个数字，能组成多少个互不相同且无重复数字的三位数？都是多少？
int main()
{
	int a, b, c,count;
	for (a = 1,count = 0; a <= 4; a++)
	{
		for (b = 1; b <= 4; b++)
		{
			for (c = 1; c <= 4; c++)
			{
				if (a != b && a != c && b != c)
				{
					printf("第%d个为:%d%d%d\n",++count,a,b,c);
				}
			}
		}
	}
	return 0;
}
